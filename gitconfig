[user]
	name = agupta
	email = agupta@etsy.com
[rerere]
	enabled = 1
[branch "master"]
	remote = origin
	merge = refs/heads/master
[alias]
	br = branch
	ci = commit -a
	cam = commit -am
	co = checkout
	df = diff
	dwc = diff -w -M --color-words
	lg = log
	ll = log -p
	rb = rebase
	fix = rebase -i origin/master
	rbc = rebase --continue
	re = remote
	st = status
	tree = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
	up = pull --rebase
	master-rebase = "!git fetch origin master && git rebase master"
[color]
	ui=auto
